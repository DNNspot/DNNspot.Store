CREATE VIEW {databaseOwner}[{objectQualifier}vDNNspot_Store_ShippingRateWeight]
AS
SELECT
method.StoreId
,method.Id as ShippingMethodId
,method.Name as ShippingMethodName
,rate.Id as ShippingRateWeightId
,rate.MinRange
,rate.MaxRange
,rate.ShippingCost
FROM {databaseOwner}{objectQualifier}DNNspot_Store_ShippingMethod method
INNER JOIN {databaseOwner}{objectQualifier}DNNspot_Store_ShippingRate_Weight rate ON method.Id = rate.ShippingMethodId
GO
