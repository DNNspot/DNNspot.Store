CREATE VIEW {databaseOwner}[{objectQualifier}vDNNspot_Store_ProductsSoldCounts]
AS
SELECT
ProductId
,COUNT(ProductId) as NumSold
FROM {databaseOwner}{objectQualifier}DNNspot_Store_OrderItem oi
GROUP BY ProductId
GO
/****** Object:  View {databaseOwner}[{objectQualifier}vDNNspot_Store_MainProductPhoto]    Script Date: 01/19/2010 23:12:23 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE VIEW {databaseOwner}[{objectQualifier}vDNNspot_Store_MainProductPhoto]
AS
WITH NumberedProductPhotos
AS
(
SELECT
photo.ProductId
,photo.Filename
,photo.SortOrder
,ROW_NUMBER() OVER (PARTITION BY photo.ProductId ORDER BY photo.SortOrder ASC) AS PhotoNumber
FROM {databaseOwner}{objectQualifier}DNNspot_Store_Product p
INNER JOIN {databaseOwner}{objectQualifier}DNNspot_Store_ProductPhoto photo ON p.Id = photo.ProductId
)
SELECT ProductId, [Filename] as MainPhotoFilename FROM NumberedProductPhotos WHERE PhotoNumber = 1
GO
/****** Object:  View {databaseOwner}[{objectQualifier}vDNNspot_Store_CartItemProductInfo]    Script Date: 01/19/2010 23:12:23 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE VIEW {databaseOwner}[{objectQualifier}vDNNspot_Store_CartItemProductInfo]
AS
SELECT
ci.*
,mainPhoto.MainPhotoFilename
,p.Name as ProductName
,p.Slug as ProductSlug
,p.Price as ProductItemPrice
,p.[Weight] as ProductWeight
,p.ShippingAdditionalFeePerItem as ProductShippingAdditionalFeePerItem
FROM {databaseOwner}{objectQualifier}DNNspot_Store_CartItem ci
INNER JOIN {databaseOwner}{objectQualifier}DNNspot_Store_Product p ON ci.ProductId = p.Id
LEFT JOIN {databaseOwner}{objectQualifier}vDNNspot_Store_MainProductPhoto mainPhoto ON p.Id = mainPhoto.ProductId
GO
